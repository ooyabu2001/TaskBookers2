
<%= render "books/book_user",book: @book %>
<div class="col-md-8 offset-md-1">
<h1>Users</h1>
<table class="table">
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <tr>
      <td> <p><%=  image_tag current_user.get_profile_image(100, 100) %></p></td>
      <td><%=current_user.name %> </td>
      <td><%= link_to "Show",user_path(current_user) %></td>
    </tr>
  </tbody>

</table>
</div>